public class Referee implements Runnable{
	
	private Queue queue;
	private Player p;
	private Player[] players;
	
	public Referee(Queue queue, Player p){
		
		this.queue = queue;
		this.p = p;
		players = new Player[2];
		
	}

	@Override
	public void run() {
	
		try {
				queue.getBlockingQueue().contains();
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
	}
				
}
